                   ELSE                                         ; Otherwise, 24 MHz is selected... raise error
                        ERROR_PSoC SEE COMMENTS                 ; Error - cannot use 24 MHz w/ 3.3V operation (Clock <= 12 MHz)
                   ENDIF
                 ENDIF
